{
    "expo": {
      "name": "rn-banking-trial",
      "slug": "rn-banking-trial",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/images/icon.png",
      "scheme": "rnbankingtrial",
      "userInterfaceStyle": "automatic",
      "newArchEnabled": true,
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.tamilarasann007.rnbankingtrial",
        "buildNumber": "1",
        "icon": "./assets/images/icon.png",
        "infoPlist": {
          "NSCameraUsageDescription": "This app uses the camera for biometric authentication.",
          "NSFaceIDUsageDescription": "This app uses Face ID for secure authentication.",
          "NSMicrophoneUsageDescription": "This app may use the microphone for enhanced security features.",
          "ITSAppUsesNonExemptEncryption": false
        }
      },
      "android": {
        "adaptiveIcon": {
          "backgroundColor": "#E6F4FE",
          "foregroundImage": "./assets/images/android-icon-foreground.png",
          "backgroundImage": "./assets/images/android-icon-background.png",
          "monochromeImage": "./assets/images/android-icon-monochrome.png"
        },
        "edgeToEdgeEnabled": true,
        "predictiveBackGestureEnabled": false,
        "package": "com.tamilarasann007.rnbankingtrial"
      },
      "web": {
        "output": "static",
        "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
        "expo-router",
        [
          "expo-splash-screen",
          {
            "image": "./assets/images/splash-icon.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "backgroundColor": "#ffffff",
            "dark": {
              "backgroundColor": "#000000"
            }
          }
        ],
        "expo-secure-store"
      ],
      "experiments": {
        "typedRoutes": true,
        "reactCompiler": true
      },
      "extra": {
        "router": {},
        "eas": {
          "projectId": "15bce151-3987-430c-976e-05702381d2ee"
        }
      }
    }
  }
  